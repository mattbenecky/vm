{
  "securityRules": [
    {
      "name": "SSH",
      "properties": {
        "access": "Allow",
        "description": "Allow Inbound SSH from GitHub-hosted Runner.",
        "destinationAddressPrefix": "*",
        "destinationPortRange": "22",
